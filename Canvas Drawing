<script>

var c = document.getElementById("myCanvas");
var ctx = c.getContext("2d");
//eye 1//
ctx.moveTo(50,100);
ctx.lineTo(50,200);
ctx.lineTo(150,200);
ctx.lineTo(150,100);

ctx.moveTo(75,125);
ctx.lineTo(75,175);
ctx.lineTo(125,175);
ctx.lineTo(125,125);
ctx.lineTo(75,125);

ctx.fillStyle = "";
ctx.fillRect(75,125,50,50);

//eyebrow 1//
ctx.moveTo(50,75);
ctx.lineTo(80,50);
ctx.lineTo(150,75);
ctx.moveTo(50,80);
ctx.lineTo(80,55);
ctx.lineTo(150,80);

//eye 2//
ctx.moveTo(350,100);
ctx.lineTo(350,200);
ctx.lineTo(450,200);
ctx.lineTo(450,100);

ctx.moveTo(375,125);
ctx.lineTo(375,175);
ctx.lineTo(425,175);
ctx.lineTo(425,125);
ctx.lineTo(375,125);

ctx.fillStyle = "";
ctx.fillRect(375,125,50,50);

//eyebrow 2//
ctx.moveTo(350,75);
ctx.lineTo(360,110);
ctx.lineTo(450,75);
ctx.moveTo(355,70);
ctx.lineTo(365,100);
ctx.lineTo(455,65);

//nose//

ctx.moveTo(220,150);
ctx.lineTo(200,300);
ctx.lineTo(185,305);
ctx.lineTo(185,320);
ctx.lineTo(195,330);
ctx.lineTo(220,330);
ctx.lineTo(225,340);
ctx.lineTo(275,340);
ctx.lineTo(275,340);
ctx.lineTo(280,330);
ctx.lineTo(305,330);
ctx.lineTo(315,320);
ctx.lineTo(315,305);
ctx.lineTo(300,300);
ctx.lineTo(280,150);

//mouth//

ctx.moveTo(125,390);
ctx.lineTo(175,410);
ctx.lineTo(325,410);
ctx.lineTo(375,390);
ctx.lineTo(275,380);
ctx.lineTo(265,385);
ctx.lineTo(235,385);
ctx.lineTo(225,380);
ctx.lineTo(125,390);
ctx.lineTo(175,430);
ctx.lineTo(325,430);
ctx.lineTo(375,390)

